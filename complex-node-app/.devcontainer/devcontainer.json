{
    "name": "complex-node",
    "dockerFile": "Dockerfile",
    "appPort": [3000],
    "runArgs": ["-u", "node"],
    "settings": {
        "workbench.colorTheme": "Night Owl",
        "terminal.integrated.shell.linux": "/bin/bash",
    },
    "postCreateCommand": "npm i",
    "extensions": [
        "sdras.night-owl",
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode"
    ]
}